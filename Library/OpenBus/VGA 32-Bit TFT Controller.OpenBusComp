object VGA32_TFT: TOpenBusLibraryComponent
  Kind = obckPeripheral
  LongName = 'VGA 32-Bit TFT Controller'
  Description.Strings = (
    
      'The VGA32_TFT provides a simple, 32-bit interface between a host' +
      ' processor and a TFT (Thin Film Transistor) LCD panel. The Contr' +
      'oller fetches 16bpp-formatted data from external video memory, p' +
      'rocesses it and displays it on the connected TFT LCD panel. All ' +
      'control signals required by the panel are generated in-core, usi' +
      'ng an integrated TFT Timing Controller unit.  The Controller sup' +
      'ports a fixed TFT screen resolution of 240x320 and a fixed refre' +
      'sh rate of 50Hz.')
